#NEXUS
begin MrBayes;
      execute albanoRomance_sc_cc.nex;
      charset sc = 1-1440;
      charset cc = 1441-1626;
      partition dtype = 2:sc, cc;
      set partition = dtype;
      unlink Statefreq=(all) shape=(all);
      lset applyto=(all) rates=gamma;
      lset applyto=(1) coding=all;
      lset applyto=(2) coding=noabsencesites;
      prset brlenspr = clock:uniform;
      prset clockvarpr = igr;
      constraint romance = ARAGONESE ARPITAN ASTURIAN BALEAR_CATALAN CATALAN CATALAN_2 CORSICAN DALMATIAN EMILIANO_CARPIGIANO EMILIANO_FERRARESE EMILIANO_REGGIANO FRENCH FRIULIAN GALICIAN GASCON ITALIAN ITALIAN_GROSSETO_TUSCAN JUDEO_ESPAGNOL LANGUEDOCIEN LIGURIAN_GENOESE LIGURIAN_RAPALLO LIGURIAN_STELLA LOMBARD_BERGAMO LOMBARD_PLESIO NEAPOLITAN_CALABRESE NONES NONES_FASSANO NONES_GARDENESE OCCITAN_ARANESE PIEMONTESE_BARBANIA PIEMONTESE_CARMAGNOLA PIEMONTESE_LANZO_TORINESE PIEMONTESE_VERCELLESE PORTUGUESE ROMAGNOL_RAVENNATE ROMANIAN ROMANIAN_2 ROMANIAN_MEGLENO ROMANSH_GRISHUN ROMANSH_SURMIRAN ROMANSH_SURSILVAN ROMANSH_VALLADER SARDINIAN SARDINIAN_CAMPIDANESE SARDINIAN_LOGUDARESE SICILIAN_UnnamedInSource SPANISH TURIA_AROMANIAN VALENCIAN VLACH;
      prset topologypr = constraints(romance);
      report applyto=(2) ancstates=yes;
      mcmcp stoprule=no stopval = 0.01 filename = romance nruns=4;
      mcmcp mcmcdiagn=yes diagnfreq=10000 samplefreq=10000 burninfrac=.5;
      set seed=12345;
      set swapseed=12345;
      mcmc ngen = 50000000;
      sumt;
      sump;
end;